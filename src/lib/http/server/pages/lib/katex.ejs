<% const cdnKaTeXURL = "https://cdn.bootcdn.net/ajax/libs/KaTeX/0.13.0" %>
<link href="<%- cdnKaTeXURL %>/katex.min.css" rel="stylesheet">
<script src="<%- cdnKaTeXURL %>/katex.min.js"></script>
<script src="<%- cdnKaTeXURL %>/contrib/auto-render.min.js"></script>
<script>
__ONLOADS__.push(function() {
    for (let ele of document.querySelectorAll("code[class*='language-latex']")) {
        ele.parentElement.innerHTML = katex.renderToString(ele.innerText, { throwOnError: false, displayMode: true, leqno: false });
    }
    for (let par of document.querySelectorAll('p,li')) {
        renderMathInElement(par, {
            delimiters: [
                { left: "$$", right: "$$", display: true },
                { left: "$", right: "$", display: false }
            ]
        });
    }
});
</script>